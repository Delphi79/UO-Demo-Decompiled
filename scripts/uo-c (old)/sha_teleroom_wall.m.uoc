// UO-C (old)
include globals;

on message<"vanish">(obj sender, list args)
{
  integer Q5A2 = getObjType(this);
  if(Q5A2 == 0x01)
  {
    return(0x00);
  }
  else
  {
    setObjVar(this, "vanished", Q5A2);
    setType(this, 0x01);
    callback(this, 0x1E, 0x24);
  }
  return(0x00);
}

on callback<0x24>()
{
  integer Q5A2 = getObjVar(this, "vanished");
  setType(this, Q5A2);
  return(0x00);
}